.idea/
.vscode/
*.sublime-project
*.sublime-workspace
.project
.settings/
*.komodoproject
.komodotools/
nbproject/
.buildpath
.classpath

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
desktop.ini

